<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".chef.ChefPlacedOrdersActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/chefPlacedOrders_Toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="0dp"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:titleTextAppearance="@style/CommonToolbarTitleTheme"/>

    <TextView
        android:id="@+id/noPlacedOrders_TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:text="@string/noPlacedOrders_TextView"
        android:textColor="@color/theme_dark_color"
        android:textSize="18sp" />
</RelativeLayout>